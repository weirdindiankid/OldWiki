{
  "manifest_version": 2,
  "name": "OldWiki",
  "version": "1.2",  // Incrementing from 1.1 to 1.2
  "description": "Forces Wikipedia on desktop to use the old vector skin.",
  "icons": {
      "48": "icons/oldwiki-48.png",
      "96": "icons/oldwiki-96.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.wikipedia.org/*"],
      "js": ["oldwiki.js"]
    }
  ]
}
